    <script type="text/javascript">
        $(document).ready(function(){
            /*check and set cookie for old browser bar */
            var cookiebrowser = readCookie('hfconfb');
            $notificationBar = $('.oldbrowser');
            //check for cookie
            if(!cookiebrowser){
                $notificationBar.find('.confirmCookie').click(function(){ //click to close
                    createCookie('hfconfb','1',365);
                    $notificationBar.slideUp(); //slideup
                    setTimeout(function() {
                        $notificationBar.remove(); //remove the element
                    }, 1000);
                });
            }
        });
    </script>

